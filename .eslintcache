  firebase.auth().signInWithEmailAndPassword(email, password)\r\n        .then(()=>{\r\n            history.push(from);\r\n            //am not so sure if am listening to sigin and out here, but i will check later;\r\n            //i have to redirect to the right page;\r\n        })\r\n        .catch((err)=>{\r\n            setError(err.message);\r\n        })\r\n    }\r\n\r\n    return (\r\n        <Form>\r\n            <Form.Title> Sign in MyApp</Form.Title>\r\n            <Form.Card>\r\n                <Form.Base method=\"POST\" onSubmit={HandleSignin}>\r\n                    <Form.Input\r\n                        onChange={({ target }) => setEmail(target.value)}\r\n                        type=\"email\"\r\n                        placeholder=\"Email address\" />\r\n                    <Form.Input\r\n                        onChange={({ target }) => setPassword(target.value)}\r\n                        type=\"password\"\r\n                        placeholder=\"password\" />\r\n                    <Form.Button type=\"submit\" marginTop={true} >Sign Up</Form.Button>\r\n                </Form.Base>\r\n            </Form.Card>\r\n            <Form.Card>\r\n                <Form.Text>New to the MyApp? <Form.Link to=\"/sign-up\">Create an Account</Form.Link></Form.Text>\r\n            </Form.Card>\r\n        </Form>\r\n    )\r\n}","C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\form\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\form\\style\\form.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\globalStyles\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\globalStyles\\variables.jsx",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\navSeach\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpa
ange={({ target }) => setPassword(target.value)}\r\n                        type=\"password\"\r\n                        placeholder=\"password\" />\r\n                    <Form.Button type=\"submit\" marginTop={true} >Sign Up</Form.Button>\r\n                </Form.Base>\r\n            </Form.Card>\r\n            <Form.Card>\r\n                <Form.Text>New to the MyApp? <Form.Link to=\"/sign-up\">Create an Account</Form.Link></Form.Text>\r\n            </Form.Card>\r\n        </Form>\r\n    )\r\n}","C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\form\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\form\\style\\form.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\globalStyles\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\globalStyles\\variables.jsx",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\navSeach\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\navbar\\index.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\components\\navbar\\style\\navbar.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\context\\firebase.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\container\\navbar.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\lib\\firebase.js",["141"],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\seedData.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\container\\browse.js",[],"C:\\Users\\Marindi k\\OneDrive\\Desktop\\WorkSpace\\books-app\\src\\hook\\content.js",["142"],"import { firebase } from '../lib/firebase';\r\nimport { useState, useEffect } from 'react';\r\n\r\nexport default function